{
  "name": "guru-vscode-theme",
  "displayName": "Guru Theme",
  "description": "VSCode theme inspired from Github VSCode theme and IntelliJ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Guru Light",
        "uiTheme": "vs",
        "path": "./themes/light-color-theme.json"
      }
    ]
  },
  "keywords": [
    "theme",
    "github",
    "intellij",
    "light"
  ],
  "devDependencies": {
    "@primer/primitives": "^4.5.1",
    "chroma-js": "^2.1.2",
    "color": "^3.1.3",
    "nodemon": "^2.0.8"
  },
  "scripts": {
    "start": "nodemon --watch src src/index.js",
    "build": "node src/index.js"
  }
}
