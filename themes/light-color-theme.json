    "panelTitle.inactiveForeground": "#6a737d",
    "panelInput.border": "#e1e4e8",
    "terminal.foreground": "#586069",
    "gitDecoration.addedResourceForeground": "#28a745",
    "gitDecoration.modifiedResourceForeground": "#005cc5",
    "gitDecoration.deletedResourceForeground": "#d73a49",
    "gitDecoration.untrackedResourceForeground": "#28a745",
    "gitDecoration.ignoredResourceForeground": "#959da5",
    "gitDecoration.conflictingResourceForeground": "#e36209",
    "gitDecoration.submoduleResourceForeground": "#959da5",
    "debugToolBar.background": "#fff",
    "editor.stackFrameHighlightBackground": "#ffd33d33",
    "editor.focusedStackFrameHighlightBackground": "#28a74525",
    "settings.headerForeground": "#2f363d",
    "settings.modifiedItemIndicator": "#2188ff",
    "welcomePage.buttonBackground": "#f6f8fa",
    "welcomePage.buttonHoverBackground": "#e1e4e8"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "*.static": {
      "fontStyle": "italic"
    }
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8C8C8C"
      }
    },
    {
      "name": "Comments: Single line",
      "scope": ["comment.line.double-slash"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "fontStyle": "",
        "foreground": "#8C8C8C"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": {
        "foreground": "#8C8C8C"
      }
    },
    {
      "name": "Comments: Storage class",
      "scope": ["storage.type.class.jsdoc"],
      "settings": {
        "foreground": "#999999",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Comments: Variables",
      "scope": ["variable.other.jsdoc"],
      "settings": {
        "foreground": "#3D3D3D"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator", "storage.type.function.arrow"],
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Keywords & Types",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "support.type",
        "variable.language.this",
        "keyword.operator.new",
        "keyword.operator.expression",
        "variable.language.super",
        "constant.language.boolean",
        "variable.language.java"
      ],
      "settings": {
        "foreground": "#0033B3"
      }
    },
    /* {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": {
        "foreground": "#7A3E9D"
      }
    }, */
    /* {
      "name": "Language Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#AB6526"
      }
    }, */
    {
      "name": "Constants",
      "scope": ["variable.other.constant", "variable.other.object.property.java"],
      "settings": {
        "foreground": "#871094",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable.other.readwrite"],
      "settings": {
        "foreground": "#080808"
      }
    },
    {
      "name": "Variable: Instance",
      "scope": ["variable.object.property", "meta.object-literal.key"],
      "settings": {
        "foreground": "#871094"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#00627A"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.type", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#080808"
      }
    },
    {
      "name": "Packages",
      "scope": ["storage.modifier.import", "storage.modifier.package"],
      "settings": {
        "foreground": "#080808"
      }
    },
    {
      "name": "Enums",
      "scope": ["variable.other.enummember"],
      "settings": {
        "foreground": "#871094",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": ["constant.numeric", "constant.character"],
      "settings": {
        "foreground": "#1750EB"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#c24e00"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#032f62"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#032f62"
      }
    },
    {
      "scope": ["string.regexp punctuation.definition.string.begin", "string.regexp punctuation.definition.string.end"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f66a0a"
      }
    },
    {
      "scope": "string.regexp constant.other.character-class.range.regexp",
      "settings": {
        "foreground": "#067D17"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#22863a"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#AB6526"
      }
    },
    {
      "name": "Annotations",
      "scope": ["meta.decorator", "storage.type.annotation"],
      "settings": {
        "foreground": "#9E880D"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.definition.annotation"],
      "settings": {
        "foreground": "#9E880D"
      }
    },
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#8C8C8C"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#1b1f23"
      }
    },
    {
      "name": "HTML: Text",
      "scope": "text.html",
      "settings": {
        "foreground": "#1b1f23"
      }
    },
    {
      "name": "HTML: Tag Names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#0033B3"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": ["meta.tag entity.other.attribute-name", "entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#174AD4"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": ["constant.character.entity", "punctuation.definition.entity.html"],
      "settings": {
        "foreground": "#c24e00"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": ["meta.selector", "meta.selector entity", "meta.selector entity punctuation", "entity.name.tag"],
      "settings": {
        "foreground": "#0033B3"
      }
    },
    {
      "name": "CSS: ID and class selectors",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#871094"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#174AD4"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": ["meta.property-value", "meta.property-value constant.other", "support.constant.property-value"],
      "settings": {
        "foreground": "#448C27"
      }
    },
    {
      "name": "CSS: Units",
      "scope": ["keyword.other.unit"],
      "settings": {
        "foreground": "#c24e00"
      }
    },
    {
      "name": "SCSS: Variables",
      "scope": ["variable.scss"],
      "settings": {
        "foreground": "#c24e00"
      }
    },
    {
      "name": "SCSS: Tag reference",
      "scope": ["entity.name.tag.reference"],
      "settings": {
        "foreground": "#0033B3"
      }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Error",
      "scope": "markup.error",
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": {
        "foreground": "#005cc5"
      }
    },
    {
      "name": "Markup: Output",
      "scope": ["markup.output", "markup.raw"],
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Markup: Prompt",
      "scope": "markup.prompt",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#AA3731"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Traceback",
      "scope": "markup.traceback",
      "settings": {
        "foreground": "#660000"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#7A3E9D"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#005cc5"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#448C27"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#AB6526"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "foreground": "#434343"
      }
    }
  ]
}
